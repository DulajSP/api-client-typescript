{
  "extends": [
    "tslint:recommended",
    "tslint-config-prettier"
  ],
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "node_modules/**/*.ts",
      "coverage/**/*.js"
    ]
  },
  "rules": {
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "jsx-no-lambda": false,
    "interface-name": false,
    "no-reference": false,
    "no-console": false,
    "member-access": false,
    "member-ordering": false,
    "no-unused-expression": [
      true,
      "allow-tagged-template"
    ],
    "variable-name": false
  }
}